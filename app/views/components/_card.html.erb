  
  

  
<div class="card-category d-flex d-flex justify-content-around">        
    <% @cocktails.each do |cocktail| %>
      <p  
        ><%= link_to cocktail.name, cocktail_path(cocktail) %>
        <%= cl_image_tag cocktail.photo.key, height: 200, width: 200, crop: :fill  %>
      </p>
    <% end %>
</div>